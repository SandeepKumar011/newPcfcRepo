{
  "name": "playwright-js-bdd-accelerator",
  "version": "1.0.0",
  "description": "This package helps to create a basic folder structure for Playwright JS with BDD",
  "scripts": {
    "test": "npx bddgen && npx playwright test",
    "testHead":"npx bddgen && npx playwright test --workers=1 --headed --project=chromium",
    "regPack":"npx bddgen && npx playwright test --workers=1 --headed --project=chromium features/regressionPack/",
    "sanityPack":"npx bddgen && npx playwright test --workers=1 --headed --project=chromium features/sanity/",
    "openReport":"npx playwright show-report"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^9.2.0",
    "@faker-js/faker": "^8.0.2",
    "@playwright/test": "^1.35.0",
    "allure-playwright": "^2.4.0",
    "dayjs": "^1.11.7",
    "libphonenumber-js": "^1.10.37",
    "playwright": "^1.35",
    "playwright-bdd": "^8.0.1",
    "test-results-reporter": "^1.0.17"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nitor-infotech-oss/PlaywrightJs-BDD-Accelerator.git"
  },
  "keywords": [
    "JS",
    "Playwright",
    "BDD"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nitor-infotech-oss/PlaywrightJs-BDD-Accelerator/issues"
  },
  "homepage": "https://github.com/nitor-infotech-oss/PlaywrightJs-BDD-Accelerator#readme",
  "dependencies": {
    "@playwright/browser-chromium": "^1.49.1",
    "cjs": "^0.0.11",
    "cjs-loader": "^0.1.0",
    "cli": "^1.0.1",
    "playwright-firefox": "^1.49.1",
    "playwright-webkit": "^1.49.1"
  }
}
